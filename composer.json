{
    "name": "gpupo/common",
    "description": "Common Objects",
    "homepage": "https://opensource.gpupo.com/common/",
    "authors": [
        {
            "name": "Gilmar Pupo",
            "email": "contact@gpupo.com"
        }
    ],
    "keywords": ["common"],
    "license": "MIT",
    "require": {
        "php": "^7.3.1",
        "symfony/console": "^4.2.2",
        "symfony/var-dumper": "^4.2.2",
        "symfony/lock": "4.2.2",
        "symfony/dotenv": "4.2.2",
        "monolog/monolog": "^1.23",
        "symfony/cache": "^4.2.2",
        "twig/twig": "^2.4",
        "psr/simple-cache": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Gpupo\\Common\\": "src",
            "Gpupo\\Common\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "phan/phan": "^1.0"
